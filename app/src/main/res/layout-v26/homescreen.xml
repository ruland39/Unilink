<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomescreenActivity">

    <TextView
        android:id="@+id/UnilinkTitleText0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:font="@font/nunitoblack"
        android:fontFamily="@font/nunitoblack"
        android:text="@string/app_name"
        android:textAlignment="center"
        android:textColor="@color/purple_main"
        android:textSize="20sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/collapseMenuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="764dp"
        android:background="@null"
        android:contentDescription="@string/collapsemenubutton"
        app:layout_constraintBottom_toTopOf="@+id/botnavcontainer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/collapsemenubutton" />

    <ImageView
        android:id="@+id/botnavcontainer"
        android:layout_width="413dp"
        android:layout_height="0dp"
        android:layout_marginBottom="3dp"
        android:contentDescription="@string/bottomnavbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/collapseMenuButton"
        app:srcCompat="@drawable/botnavbarcontainer" />

    <ImageButton
        android:id="@+id/homebuttonactive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="46dp"
        android:background="@null"
        android:contentDescription="@string/homebuttonactive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/chatbuttoninactive"
        app:layout_constraintTop_toTopOf="@+id/botnavcontainer"
        app:srcCompat="@drawable/homebutton_active" />


    <ImageButton
        android:id="@+id/chatbuttoninactive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="46dp"
        android:background="@null"
        android:contentDescription="@string/chatbuttoninactive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/notificationbuttoninactive"
        app:layout_constraintTop_toTopOf="@+id/botnavcontainer"
        app:srcCompat="@drawable/chatbutton_inactive" />

    <ImageButton
        android:id="@+id/notificationbuttoninactive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="46dp"
        android:background="@null"
        android:contentDescription="@string/notificationbuttoninactive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botnavcontainer"
        app:srcCompat="@drawable/notificationbutton_inactive" />

    <ImageButton
        android:id="@+id/profilebuttoninactive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="46dp"
        android:background="@null"
        android:contentDescription="@string/profilebuttoninactive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/notificationbuttoninactive"
        app:layout_constraintTop_toTopOf="@+id/botnavcontainer"
        app:srcCompat="@drawable/profilebutton_inactive" />

    <ImageView
        android:id="@+id/bannercontainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/mainbanner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UnilinkTitleText0"
        app:srcCompat="@drawable/bannercontainer" />

    <!--    Button Container 1 Start -->

    <ImageButton
        android:id="@+id/bluetoothbuttondefault"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="136dp"
        android:layout_marginBottom="528dp"
        android:background="@null"
        android:contentDescription="@string/bluetoothbuttondefault"
        app:layout_constraintBottom_toTopOf="@+id/botnavcontainer"
        app:layout_constraintStart_toStartOf="@+id/bannercontainer"
        app:layout_constraintTop_toTopOf="@+id/bannercontainer"
        app:srcCompat="@drawable/bluetoothbuttondefault" />

    <ImageView
        android:id="@+id/maincontainer1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/maincontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bluetoothbuttondefault"
        app:srcCompat="@drawable/maincontainer" />

    <TextView
        android:id="@+id/personusernametextviewmaincontainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="476dp"
        android:fontFamily="@font/nunitobold"
        android:text="@string/personusernametextviewmaincontainer"
        android:textColor="@color/almost_black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/botnavcontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/maincontainer1"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/aboutsectiontextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/nunitoregular"
        android:text="@string/personabouttextviewmaincontainer"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/personusernametextviewmaincontainer"
        app:layout_constraintTop_toBottomOf="@+id/personusernametextviewmaincontainer" />

    <ImageButton
        android:id="@+id/defaultprofilepicturesmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:background="@null"
        android:contentDescription="@string/defaultprofilepicturesmall"
        app:layout_constraintStart_toStartOf="@+id/maincontainer1"
        app:layout_constraintTop_toTopOf="@+id/maincontainer1"
        app:srcCompat="@drawable/defaultprofilepicturesmall" />


    <!--    Button Container 1 End -->

    <ImageButton
        android:id="@+id/wavebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="16dp"
        android:background="@null"

        android:contentDescription="@string/wavesmallbutton"
        app:layout_constraintBottom_toBottomOf="@+id/maincontainer1"
        app:layout_constraintEnd_toEndOf="@+id/maincontainer1"
        app:srcCompat="@drawable/wavesmallbutton" />

    <ImageButton
        android:id="@+id/chatsmallbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="16dp"
        android:background="@null"

        android:contentDescription="@string/chatsmallbutton"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/maincontainer1"
        app:layout_constraintEnd_toEndOf="@+id/maincontainer1"
        app:srcCompat="@drawable/chatsmallbutton" />

    <ImageView
        android:id="@+id/maincontainer2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/maincontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maincontainer1"
        app:srcCompat="@drawable/maincontainer" />

    <ImageView
        android:id="@+id/maincontainer3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/maincontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maincontainer2"
        app:srcCompat="@drawable/maincontainer" />

    <ImageView
        android:id="@+id/maincontainer4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/maincontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maincontainer3"
        app:srcCompat="@drawable/maincontainer" />

    <ImageView
        android:id="@+id/maincontainer5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/maincontainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maincontainer4"
        app:srcCompat="@drawable/maincontainer" />

</androidx.constraintlayout.widget.ConstraintLayout>